        &::before
          background-image url(../images/icon-3.svg)
    &:nth-of-type(4)
      a
        &::before
          background-image url(../images/icon-4.svg)
    &:nth-of-type(5)
      a
        &::before
          background-image url(../images/icon-5.svg)
  a
    position relative
    display block
    height 100%
