.parallax
  -webkit-backface-visibility: hidden
  backface-visibility: hidden
  *
    -webkit-backface-visibility: hidden
    backface-visibility: hidden

  perspective: 100px
  height: 100vh
  overflow-x: hidden
  overflow-y: auto
  position: absolute
  width 100%
  top: 0
  //left: 50%
  right: 0
  bottom: 0
  //margin-left: -1500px
.parallax__layer
  position: absolute
  top: 0
  right: 0
  bottom: 0
  left: 0
  background-repeat no-repeat !important
  .layer,
  img
    background-repeat no-repeat
    display: block
    position: absolute
    bottom: 0

.parallax__cover
  background: #111
  position: absolute
  top: 100%
  left: 0
  right: 0
  //height: 2000px
  z-index: 2











$parallax__layers = 6
for $i in (0)..($parallax__layers)
  $x = ($parallax__layers - $i) / 10
  .parallax__layer__{$i}
    transform: translateZ((-100 * $x)px) scale($x + 1)








